#include<iostream>
using namespace std;

class Person {
    public:
        string firstName;
        string lastName;
        int age;

        void fullname() {
            cout << this->firstName << ' ' << Person::lastName;
        }
};

int main() {

Person person;
person.firstName = “Duc Anh";
person.lastName = “Nguyen";
person.fullname(); // s? in ra màn hình là “nguyen duc anh"
	return 0;
}

